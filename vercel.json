{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/[[...slug]]"
    },
    {
      "src": "/health",
      "dest": "/api/[[...slug]]"
    }
  ],
  "functions": {
    "api/[[...slug]].ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
